{
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES2020",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "useDefineForClassFields": false,
      "downlevelIteration": true,
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./dist",
      "rootDir": "./src"
    }
